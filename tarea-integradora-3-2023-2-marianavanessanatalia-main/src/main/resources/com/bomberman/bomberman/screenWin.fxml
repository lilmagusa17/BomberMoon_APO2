<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<AnchorPane fx:id="ap" prefHeight="600.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bomberman.bomberman.control.ScreenWinController">
    <children>
        <Canvas fx:id="header" height="60.0" layoutX="0.0" width="720.0" />
        <Canvas fx:id="canvas" height="600.0" layoutX="0.0" layoutY="0.0" width="720.0" />
        <ImageView fitHeight="600.0" fitWidth="720.0" layoutY="0.0" pickOnBounds="true" preserveRatio="false">
            <image>
                <Image url="@img/background/WinScreen.png" />
            </image></ImageView>
        <ImageView fitHeight="600.0" fitWidth="720.0" layoutX="0.0" layoutY="0.0" opacity="0.17" pickOnBounds="false">
            <image>
                <Image url="@img/background/starsBg2.gif" />
            </image></ImageView>

        <Button fx:id="exit" layoutX="56.0" layoutY="492.0" mnemonicParsing="false" text="Exit" />
        <Button fx:id="again" layoutX="621.0" layoutY="492.0" mnemonicParsing="false" onAction="#returnToStartView" text="Again" />
    </children>

</AnchorPane>
